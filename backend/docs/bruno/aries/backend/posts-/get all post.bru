meta {
  name: get all post
  type: http
  seq: 1
}

get {
  url: {{protocol}}://{{host}}:{{port}}/api/v1/posts?page=1&size=10
  body: none
  auth: basic
}

params:query {
  page: 1
  size: 10
  ~email: milunes@gmail.com
}

headers {
  User-Agent: insomnia/10.2.0
  X-CSRF-TOKEN: undefined
}

auth:basic {
  username: {{api_username}}
  password: {{api_password}}
}
